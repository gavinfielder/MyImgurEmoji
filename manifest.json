{
	"manifest_version": 2,
	"name": "My First Chrome Extension",
	"description": "This is a description",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-3.4.1.js", "content.js"]
		}
	],
	"permissions": [
		"storage",
		"activeTab"
	],
	"browser_action": {
		"default_icon": "image.png",
		"default_popup": "popup.html",
		"default_title": "My First Chrome Extension"
	}
}
